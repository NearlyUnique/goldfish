{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Migrate",
            "program": "${workspaceFolder}/scripts/migrate_visits.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "cwd": "${workspaceFolder}/scripts",
            "args": [
                "--key=../.secrets/goldfish-admin.json"
            ]
        },
        {
            "name": "Goldfish (Debug)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "debug"
        },
        {
            "name": "Goldfish (Profile)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "profile"
        },
        {
            "name": "Goldfish (Release)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "release"
        },
        {
            "name": "Goldfish (Debug) - Specific Device",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "debug",
            "deviceId": "emulator-5554"
        }
    ]
}
